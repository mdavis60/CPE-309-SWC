<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<Pane  styleClass="main-panel"
        xmlns:fx="http://javafx.com/fxml" fx:controller="org.swp.scheduler.LogInController">
   <children>
      <TextField alignment="TOP_CENTER" editable="false" layoutX="113.0" layoutY="34.0" text="LOGIN">
         <font>
            <Font name="System Bold" size="31.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="237.0" layoutY="220.0" promptText="Password" />
      <TextField fx:id="usernameField" layoutX="237.0" layoutY="159.0" promptText="E-mail" />
      <Button layoutX="256.0" layoutY="342.0" mnemonicParsing="false" text="Forgot Password?" />
      <Button onAction="#validateInfo" layoutX="281.0" layoutY="287.0" mnemonicParsing="false" text="Sign In" />
   </children>
</Pane>
